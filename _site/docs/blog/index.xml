<span id="cb24-2">  <span class="fu" style="color: #4758AB;">ggplot</span>(<span class="fu" style="color: #4758AB;">aes</span>(Estimate, field, <span class="at" style="color: #657422;">fill =</span> <span class="fu" style="color: #4758AB;">stat</span>(x)))<span class="sc" style="color: #5E5E5E;">+</span></span>
<span id="cb24-3">  <span class="fu" style="color: #4758AB;">geom_density_ridges_gradient</span>(<span class="at" style="color: #657422;">alpha =</span> <span class="fl" style="color: #AD0000;">0.3</span>)<span class="sc" style="color: #5E5E5E;">+</span></span>
<span id="cb24-4">  <span class="fu" style="color: #4758AB;">scale_x_log10</span>()<span class="sc" style="color: #5E5E5E;">+</span></span>
<span id="cb24-5">  <span class="fu" style="color: #4758AB;">scale_fill_viridis_c</span>(<span class="at" style="color: #657422;">option =</span> <span class="st" style="color: #20794D;">"C"</span>)<span class="sc" style="color: #5E5E5E;">+</span></span>
<span id="cb24-6">  <span class="fu" style="color: #4758AB;">facet_wrap</span>(<span class="sc" style="color: #5E5E5E;">~</span>fungicida, <span class="at" style="color: #657422;">nrow =</span> <span class="dv" style="color: #AD0000;">2</span>)<span class="sc" style="color: #5E5E5E;">+</span></span>
<span id="cb24-7">  <span class="fu" style="color: #4758AB;">theme_minimal_hgrid</span>()<span class="sc" style="color: #5E5E5E;">+</span></span>
<span id="cb24-8">  <span class="fu" style="color: #4758AB;">labs</span>(<span class="at" style="color: #657422;">x =</span> <span class="st" style="color: #20794D;">"EC50"</span>, <span class="at" style="color: #657422;">y =</span> <span class="st" style="color: #20794D;">"Field"</span>)<span class="sc" style="color: #5E5E5E;">+</span></span>
